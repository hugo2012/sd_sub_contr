<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" 
xmlns:vm="sap.m.VariantManagement"
 controllerName="com.bosch.rb1m.sd.sd_subcontr.controller.Main"
xmlns:html="http://www.w3.org/1999/xhtml" xmlns:layout="sap.ui.layout.form"
xmlns:f="sap.f"
height="100%"
>		
	<f:DynamicPage id="_IDGenPage">			
	<f:title>
			<f:DynamicPageTitle id="_IDGenDynamicPageTitle">
				<f:heading>
					<VariantManagement
					id="variantManagement"					
					selectedKey="{view>/selectedKey}"
					items="{view>/variants}"
					select="onVariantSelect"
					save="onVariantSave"
					showSaveAs="true"	
					showFooter="true"									
					manage="onVariantManage">						
					<items>
						<VariantItem id="_IDGenVariantItem1"
						key="{view>key}"
						text="{view>text}"
						executeOnSelect="true"
						changeable="true"
						sharing="{= ${view>key} === 'Default' ? 'Public' : 'Private'}"
						enabled="true"
						author="{= ${view>key} === 'Default' ? 'SAP' : ${view>author}}"
						remove="{= ${view>key} === 'Default' ? false : true }"
						rename="{= ${view>key} === 'Default' ? false : true }"	
							/>
					</items>
					</VariantManagement>
				</f:heading>
			</f:DynamicPageTitle>
		</f:title>		     	
		<f:content> 		
			<VBox id="_IDGenVBox" class="sapUiSmallMargin">
				<layout:SimpleForm id="_IDGenSimpleForm" editable="true" title="{i18n>formTitle}" layout="ResponsiveGridLayout"  labelSpanXL="3" labelSpanL="3"
					labelSpanM="3" labelSpanS="12" adjustLabelSpan="false" emptySpanXL="4" emptySpanL="4" emptySpanM="4" emptySpanS="0" columnsXL="1"
					columnsL="1" columnsM="1" singleContainerFullSize="false">
					<layout:content>
						 <Label id="_lbl_plant" text="{i18n>lbl_plant}" labelFor="idip_plant"/>
						<Input id="idip_plant" required="true"
						   showValueHelp="true" submit="onSubmitInput" 
						   valueHelpRequest="onValueHelpRequestAuto"
						   change="onInputChange">
						</Input>
						<Label id="lbl_shipping_point" text="{i18n>lbl_shipping_point}" labelFor="idip_shipping_point"/>
						<Input id="idip_shipping_point" required="true"
						 showValueHelp="true" submit="onSubmitInput" 
						   valueHelpRequest="onValueHelpRequestAuto"
						   change="onInputChange">
						</Input>
						<Label id="lbl_vendor" text="{i18n>lbl_vendor}" labelFor="idip_vendor"/>
						<MultiInput id="idip_vendor" 
						 showValueHelp="true" submit="onSubmitInput" 
						   valueHelpRequest="onValueHelpRequestAuto"
						   change="onInputChange" tokenUpdate="onTokenUpdate"
						   liveChange="onInputChange">
						</MultiInput>
						<Label id="lbl_purchase_oder" text="{i18n>lbl_purchase_oder}" labelFor="idip_purchase_oder"/>
						<MultiInput id="idip_purchase_oder" 
						 showValueHelp="true" submit="onSubmitInput" 
						   valueHelpRequest="onValueHelpRequestAuto"
						   change="onInputChange">
						</MultiInput>
						<Label id="lbl_req_date" text="{i18n>lbl_req_date}" labelFor="iddate_req_date"/>
						 <MultiInput id="iddate_req_date" name="Date" required="true"
									editable="true"
									enabled="true"								
									showSuggestion="false"
									showValueHelp="true"
									placeholder="DD.MM.YYYY"
									valueHelpRequest=".onVHReqDateRequested"
									change="onInputChange"
									submit="onSubmitInput" >
							</MultiInput>		
						<Label id="lbl_warehouse_number" text="{i18n>lbl_warehouse_number}" labelFor="idip_warehouse_number"/>
						<Input id="idip_warehouse_number" required="true"
						 showValueHelp="true" submit="onSubmitInput" 
						   valueHelpRequest="onValueHelpRequestAuto"
						   change="onInputChange">
						</Input>
						<Label id="lbl_storage_type" text="{i18n>lbl_storage_type}" labelFor="idip_storage_type"/>
						<MultiInput id="idip_storage_type" 
						 showValueHelp="true" submit="onSubmitInput" 
						   valueHelpRequest="onValueHelpRequestAuto"
						   change="onInputChange">
						</MultiInput>
						<Label id="lbl_storage_bin" text="{i18n>lbl_storage_bin}" labelFor="idip_storage_bin"/>
						<MultiInput id="idip_storage_bin" 
						 showValueHelp="true" submit="onSubmitInput" 
						   valueHelpRequest="onValueHelpRequestAuto"
						   change="onInputChange">
						</MultiInput>
						<Label id="lbl_assembly_prod" text="{i18n>lbl_assembly_prod}" labelFor="idip_assembly_prod"/>
						<MultiInput id="idip_assembly_prod" 
						 showValueHelp="true" submit="onSubmitInput" 
						   valueHelpRequest="onValueHelpRequestAuto"
						   change="onInputChange">
						</MultiInput>
						<Label id="lbl_component" text="{i18n>lbl_component}" labelFor="idip_component"/>
						<MultiInput id="idip_component" 
						 showValueHelp="true" submit="onSubmitInput" 
						   valueHelpRequest="onValueHelpRequestAuto"
						   change="onInputChange">
						</MultiInput>
					</layout:content>
				</layout:SimpleForm>
			</VBox>
		</f:content> 		
		<f:footer>
			<OverflowToolbar id="_IDGenBar">
				<ToolbarSpacer id="_IDGenToolbarSpacer4main"/>
				<Button id="idbtnContinueTxtn" visible="true" text="{i18n>btnContinueTxt}" type="Emphasized" press=".onHandleContinuePress"/>
			</OverflowToolbar>
		</f:footer>
	</f:DynamicPage>
</mvc:View>